{
  "name": "weatherapp",
  "version": "1.0.0",
  "description": "Basic weather app with React.js",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src test:watch",
    "start:prod": "babel-node tools/distServer.js",
    "open:src": "babel-node tools/srcServer.js",
    "test": "mocha --reporter progress tools/testSetup.js src/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node tools/build.js",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faulke/weatherapp.git"
  },
  "author": "Evan Faulk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/faulke/weatherapp/issues"
  },
  "homepage": "https://github.com/faulke/weatherapp#readme",
  "dependencies": {
    "async": "^2.1.4",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-register": "6.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "cheerio": "^0.22.0",
    "compression": "^1.6.1",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jquery": "^2.2.4",
    "jsdom": "8.5.0",
    "json-loader": "^0.5.4",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "request": "^2.79.0",
    "rimraf": "2.5.2",
    "serve-favicon": "^2.3.2",
    "style-loader": "0.13.1",
    "toastr": "^2.1.2",
    "url-loader": "0.5.7",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "1.1.1",
    "dotenv": "^2.0.0",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "5.0.1"
  }
}
